@model IdentityUser


@{ 
    ViewBag.Title = "Delete User";
}
    <h3>Delete User</h3>
<div class="row">
    <div class="col-md-12">
        <table class="table">
            <tr>

                <td>Id</td>
                <td>@Model.Id</td>
            </tr>
            <tr>
                <td>Email</td>
                <td>@Model.Email</td>
            </tr>
            <tr>
                <td colspan="2">
                    <form asp-action="DeleteConfirmed" asp-controller="Administrations"  method="post">
                        <input type="hidden" asp-for="Id" value="@Model.Id" />
                        <input type="hidden" asp-for="Email" value="@Model.Email" />
                        <input type="hidden" asp-for="UserName" value="@Model.UserName" />
                        <input class="btn btn-warning" type="submit"  value="DeleteConfirmed" />
                    </form>
                    
                </td>
            </tr>
        </table>
    </div>

</div>

