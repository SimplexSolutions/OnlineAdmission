@model IEnumerable<OnlineAdmission.Entity.StudentPaymentType>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.StudentCategory)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MeritType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PaymentType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AcademicSession)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Status)
            </th>

            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.StudentCategory.CategoryName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MeritType.MeritTypeName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PaymentType.PaymentTypeName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AcademicSession.SessionName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Status)
            </td>

            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
